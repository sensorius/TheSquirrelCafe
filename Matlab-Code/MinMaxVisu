% Enter your MATLAB code below
maxTemp = thingSpeakRead(142833,'Field',1);
minTemp = thingSpeakRead(142833,'Field',3);

% Simple text output of max and min temperature
annotation('textbox',[0.1 0.1 0.8 0.8],...
'HorizontalAlignment','center',...
'VerticalAlignment','middle',...
'LineStyle','none',...
'String',['Max. ' num2str(maxTemp) '°C' '       Min. ' num2str(minTemp) '°C' ],...
'FontSize',36);
